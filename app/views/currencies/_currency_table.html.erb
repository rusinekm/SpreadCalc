<table>
  <thead>
    <tr>
      <th>site name</th>
      <th>buy value</th>
      <th>sell value</th>
    </tr>
  </thead>
  <tbody>
    <% currency.sites.each do |site| %>
      <tr>
        <td><%= link_to site.name, site.url %></td>
        <td><%= site.buy_value(currency.id) %></td>
        <td><%= site.sell_value(currency.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<p class="small-12 columns, updated_text">last updated at: 
  <%=StatNode.time_of_last_node %>
</p>